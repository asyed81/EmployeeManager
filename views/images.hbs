<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" 
integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

<style>
img {size: cover;}
body{background: black;}
h2{color: white;}
</style>
        <h2 style="margin-left: 4%">Images <a style="margin-right: 4%" href="/images/add" class="btn btn-success pull-right">Add New Image</a> <a style="margin-right: 4%" href="/" class="btn btn-success pull-right">Home</a></h2>
        

        <hr />
<div class="row">
<div class="col-lg-6 col-md-3">


     {{#each items}}
    <div class="col-md-12">
        <img src="/images/uploaded/{{this}}" class="img-responsive thumbnail" />
    </div>
    {{else}}
    <div class="col-md-12 text-center">
            <strong>No Images Available</strong>
    </div>
     {{/each}} 
        </div> 
</div>
